rm ldken libdmr
cd ../ken
echo "Compiling ken"
ncc -c -O *.c
echo "Loading ken"
ld -x -r \
	alloc.o		awaitr.o	clock.o		fio.o\
	iget.o          main.o          malloc.o\
	nami.o          pipe.o          prf.o\
	rdwri.o         sig.o           slp.o\
	subr.o          sys1.o          sys2.o\
	sys3.o          sys4.o          sysent.o\
	proc_util.o     text.o          trap.o          table.o

mv a.out ../conf/ldken

chdir ../conf
as m40.s
mv a.out m40.o
as l.s
mv a.out l.o
ncc -c c.c

echo "compiling dmr"
rm -f libdmr
chdir ../dmr
ncc -c -O \
  bio.c             mem.c              kl.c\
  partab.c          rk.c              tty.c             pty.c\
  ports.c       imp11.c

echo "archiving dmr"
ar r ../conf/libdmr\
  kl.o              bio.o             mem.o\
  partab.o          rk.o              tty.o             pty.o\
  ports.o       imp11.c

chdir ../conf
echo Loading
ld -x l.o m40.o c.o ldken libdmr
syms -ug a.out
size a.out
mv a.out ../unix
echo 'Done - output is ../unix'

